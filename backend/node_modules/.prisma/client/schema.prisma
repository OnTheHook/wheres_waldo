generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id       Int     @id @default(autoincrement())
  name     String
  found    Boolean @default(false)
  xPercent Decimal
  yPercent Decimal
}

model Timer {
  id          Int      @id @default(autoincrement())
  userId      String   @unique
  startTime   DateTime
  elapsedTime Int
}
